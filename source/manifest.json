{

  "manifest_version": 2,
  "name": "CCEMSExt",
  "version": "1.0",

  "description": "Autofills agency name for CCEMS websites. Provides link toolbar.",

  "icons": {
    "48": "icons/ccems-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.esosuite.net/*"],
      "js": ["eso_agency.js"]
    },
    {
      "matches": ["*://*.vairkko.com/*"],
      "js": ["vairkko_agency.js"]
    }
  ]

}